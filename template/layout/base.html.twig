                        jQuery('#success').html("<div class='alert alert-danger'>");
                        jQuery('#success > .alert-danger').html("<button type='button' class='close' data-dismiss='alert' aria-hidden='true'>&times;")
                            .append("</button>");
                        jQuery('#success > .alert-danger')
                            .append("<strong>"+ data.error + "</strong>");
                        jQuery('#success > .alert-danger')
                            .append('</div>');
                        //jQuery('form[name=login]').trigger("reset");
                    }
                    if (data.suceed === true) {
                        jQuery('#success').html("<div class='alert alert-success'>");
                        jQuery('#success > .alert-success').html("<button type='button' class='close' data-dismiss='alert' aria-hidden='true'>&times;")
                            .append("</button>");
                        jQuery('#success > .alert-success')
                            .append("<strong>Lo estamos redireccionando....</strong>");
                        jQuery('#success > .alert-success')
                            .append('</div>');
                        window.location.href = 'enlinea/index.php';
                    }
                }
            });
        },				
        // debug: true,
        errorPlacement: function(error, element) {
                error.insertAfter( element );
        },
        onkeyup: false,
        onclick: false,
        rules: {
            cedula: {
                required: true
            },
            password: {
                required: true,
                minlength: 4
            }
        },
        messages: {
            cedula: {
                required: "Ingrese el código de usuario"
            },
            password: {
                required: "Ingrese su clave de acceso",
                minlength: "Debe ser mayor a 3 carácteres"
            }					
        },
        errorElement: "span",
        errorClass: "wpcf7-not-valid-tip",
        highlight: function (element) {
            jQuery(element).parent().removeClass("has-success").addClass("has-error");
            //jQuery(element).siblings("label").addClass("hide"); 
        },
        success: function (element) {
            jQuery(element).parent().removeClass("has-error").addClass("has-success");
            //jQuery(element).siblings("label").removeClass("hide"); 
        }
    });
};
});
</script>
{% endblock %}
